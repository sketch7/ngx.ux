{
  "name": "@ssv/ngx.ux",
  "version": "1.1.1",
  "description": "UX essentials for building apps, utilities which enables you writing richer apps easier.",
  "keywords": [
    "sketch7",
    "ngx",
    "angular2",
    "angular",
    "ssv",
    "ux",
    "ui",
    "viewport",
    "uiutils",
    "cdk"
  ],
  "scripts": {
    "start": "npm run build -- --watch",
    "test": "jest",
    "tdd": "jest --watch",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --env jest-environment-node-debug",
    "test-devt": "devtool ./node_modules/jest/bin/jest.js --runInBand --env jest-environment-node-debug",
    "clean": "rimraf dist",
    "lint": "tslint -p . -c tslint.json -t stylish",
    "build": "ng-packagr -p ng-package.json",
    "prepare-release": "gulp prepare-release",
    "publish:rel": "npm publish dist --access=public"
  },
  "author": "Stephen Lautier <stephen.lautier@outlook.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/sketch7/ngx.ux.git"
  },
  "dependencies": {},
  "peerDependencies": {
    "@angular/core": ">=6.0.0",
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@ssv/tools": "^0.8.1",
    "@types/jest": "^23.3.9",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-conventional-changelog": "^2.0.3",
    "jest": "^23.6.0",
    "jest-environment-node-debug": "^2.0.0",
    "ng-packagr": "^4.4.0",
    "reflect-metadata": "^0.1.12",
    "require-dir": "^1.1.0",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.2.1",
    "rxjs": "^6.3.3",
    "ts-jest": "^23.10.5",
    "tsickle": "^0.34.0",
    "tslint": "^5.11.0",
    "typescript": "~3.1.6",
    "yargs": "^12.0.5",
    "zone.js": "^0.8.26"
  },
  "resolutions": {
    "natives": "1.1.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
